<script type="text/javascript">
	/* 환율정보를 받아옴 */
	$.getJSON("/crawring", {
	},function (data) {
		setExchange(data);
	});
	
	/* 받아온 환율 값을 적용하는 함수 */
	function setExchange(data) {
		var keys = Object.keys(data);
		for(var i in keys) {
			$("#exchange_"+keys[i]).text(data[keys[i]]);
		}
	}
</script>


/*slide*/
@-webkit-keyframes imageAnimation{0%{opacity:0.8;-webkit-animation-timing-function:linear}	8%{opacity:1;-webkit-transform:scale(1.05);	-webkit-animation-timing-function:linear}	17%{opacity:1;-webkit-transform:scale(1.1)}	25%{opacity:0;-webkit-transform:scale(1.1)}	100%{opacity:0}}
@-moz-keyframes imageAnimation{0%{opacity:0.8;-moz-animation-timing-function:linear}	8%{opacity:1;-moz-transform:scale(1.05);-moz-animation-timing-function:linear}	17%{opacity:1;-moz-transform:scale(1.1)}	25%{opacity:0;-moz-transform:scale(1.1)}	100%{opacity:0}}
@-o-keyframes imageAnimation{0%{opacity:0.8;-o-animation-timing-function:linear}	8%{opacity:1;-o-transform:scale(1.05);-o-animation-timing-function:linear}	17%{opacity:1;-o-transform:scale(1.1)}	25%{opacity:0;-o-transform:scale(1.1)}	100%{opacity:0}}
@-ms-keyframes imageAnimation{0%{opacity:0.8;-ms-animation-timing-function:linear}	8%{opacity:1;-ms-transform:scale(1.05);-ms-animation-timing-function:linear}	17%{opacity:1;-ms-transform:scale(1.1)}	25%{opacity:0;-ms-transform:scale(1.1)}	100%{opacity:0}}
@keyframes imageAnimation{0%{opacity:0.8;animation-timing-function:linear}	8%{opacity:1;transform:scale(1.05);animation-timing-function:linear}	17%{opacity:1;transform:scale(1.1)}	25%{opacity:0;transform:scale(1.1)}	100%{opacity:0}}
.bg-slider { position: absolute;left: 0;top: 0;width: 100%;height: 100%;background: #000;	display:none;}
.bg-slider__item {background-image:../images/slide/slide01.jpg;position: absolute;left: 0;top: 0;width: 100%;height: 100%;-webkit-background-size: cover;background-size: cover;background-position: 50% 50%;background-repeat: no-repeat;opacity: 0;z-index: 0;-webkit-backface-visibility: hidden;-webkit-animation: imageAnimation 50s linear infinite 0s;-moz-animation: imageAnimation 50s linear infinite 0s;-o-animation: imageAnimation 50s linear infinite 0s;-ms-animation: imageAnimation 50s linear infinite 0s;animation: imageAnimation 50s linear infinite 0s;}
.bg-slider__item:nth-child(2) {background-image:../images/slide/slide02.jpg;-moz-animation-delay: 10s;-webkit-animation-delay: 10s;animation-delay: 10s;}
.bg-slider__item:nth-child(3) {background-image:../images/slide/slide03.jpg;-moz-animation-delay: 20s;-webkit-animation-delay: 20s;animation-delay: 20s;}
.bg-slider__item:nth-child(4) {background-image:../images/slide/slide04.jpg;-moz-animation-delay: 30s;-webkit-animation-delay: 30s;animation-delay: 30s;}
.bg-slider__item:nth-child(5) {background-image:../images/slide/slide05.jpg;-moz-animation-delay: 40s;-webkit-animation-delay: 40s;animation-delay: 40s;}

/* No Touch devices */
.cd-nav-trigger {
  display: none;
}
#side_nav ul{
	background: #fff;
    border: 1px solid #dedede;
    padding: 0 10px;
    }
#side_nav h2{
    background: cadetblue;
    padding: 10px;
    color: #fff;
    font-size: 14px;
    font-family: myriad Pro, Raleway, Nanum Barun Gothic ;
    text-align: center;
    text-transform: capitalize;
}
#side_nav {
  /*-webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  -o-transform: translateY(-50%);
  transform: translateY(-50%);
  z-index: 1;
  /* padding:10px; */
  background: #fff;
  border-radius:10px;
  position: absolute;
  top: 140px;
  left: 0;
  width: 160px;
  /* border: 1px solid #ccc; */
}
#side_nav li {
  text-align: left;
  border-bottom:1px solid #c8c8c8;
  padding: 10px 0;
}
#side_nav li:last-child{border:0}
#side_nav a {
  display: inline-block;/* prevent weird movements on hover when you use a CSS3 transformation - webkit browsers */
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  display: block;
  position: relative;
}
#side_nav a:after {
  content: "";
  display: table;
  clear: both;
}
#side_nav a span {
  display: inline-block;
  /*-webkit-transform: scale(0.8);
  -moz-transform: scale(0.8);
  -ms-transform: scale(0.8);
  -o-transform: scale(0.8);
  transform: scale(0.8);*/
}
#side_nav a:hover span {
  /*-webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  -o-transform: scale(1);
  transform: scale(1);*/
  font-weight:normal;
  color:#774444;
}
#side_nav a:hover .cd-label {
  opacity: 1;
}
#side_nav a.is-selected .cd-dot {
  background-color: #519CD4;;
}
#side_nav a.is-selected .cd-label{color:#519CD4}
#side_nav .cd-dot {
  position: relative;/* we set a top value in order to align the dot with the label. If you change label's font, you may need to change this top value*/
  top: 8px;
  height: 8px;
  width: 8px;
  border-radius: 50%;
  background-color: #C3C3C3;
  -webkit-transition: -webkit-transform 0.2s, background-color 0.5s;
  -moz-transition: -moz-transform 0.2s, background-color 0.5s;
  transition: transform 0.2s, background-color 0.5s;
  -webkit-transform-origin: 50% 50%;
  -moz-transform-origin: 50% 50%;
  -ms-transform-origin: 50% 50%;
  -o-transform-origin: 50% 50%;
  transform-origin: 50% 50%;
  font-family:sans-serif;
  display:none;
  float: left !important;
  display: block;
  position: absolute;
  top: 9px;
  left: 0;
}
#side_nav .cd-label {
  position: relative;
  /* margin-right: 10px; */
  /* padding: .4em .5em; */
  color: #444;
  font-size: 14px;
  -webkit-transition: -webkit-transform 0.2s, opacity 0.2s;
  -moz-transition: -moz-transform 0.2s, opacity 0.2s;
  transition: transform 0.2s, opacity 0.2s;
  opacity: 1;
  -webkit-transform-origin: 100% 50%;
  -moz-transform-origin: 100% 50%;
  -ms-transform-origin: 100% 50%;
  -o-transform-origin: 100% 50%;
  /* transform-origin: 100% 50%; */
  font-family:sans-serif;
  padding-left: 18px;
  line-height: 22px;
}

@media only screen and (max-width: 768px) {
  #side_nav{
    display: none;
  }
}